
class Thread1 extends Thread{
	int n;
	
	Thread1(int n){
		this.n = n;
	}
	public void run() {
		for(int i=1; i<n; i++) {
			 System.out.print(i);
			 if(i%10 == 0) 
				 System.out.println();
			 else System.out.print("\t");
		 	}
		 }
	}

public class Exercise {
	public static void main(String[] agrs) {
			Thread1 th = new Thread1();
	}
}
